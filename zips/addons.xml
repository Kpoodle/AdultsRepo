<?xml version="1.0" encoding="UTF-8"?>

<addons>

<addon id="plugin.video.telemedia"
	   name="Telemedia 2.0"
	   version="2.1.5"
	   provider-name="Pavel Durov">
  <requires>
	<import addon="script.module.requests"/>
	<import addon="script.module.pyxbmct" version="1.1.4" />
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.service" library="service.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="he">Telegram media for kodi,\n Thanks to "Aliaksei Levin" without him none of this would have been possiable</summary>
    <description lang="he">Telegram media for kodi</description>
    <platform>all</platform>
 <language>en</language>
 <disclaimer> report any issues with</disclaimer>
	<assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
    </assets>
  </extension>
</addon>

<addon id="repository.KodiDog" name="[B]KodiDog Add-ons Repository[/B]" version="1.0.0" provider-name="KodiDog">
	<extension point="xbmc.addon.repository" name="[B]KodiDog Add-ons Repository[/B]">
        <info>https://raw.githubusercontent.com/Kpoodle/Repo19B/master/addons.xml</info>
        <checksum>https://raw.githubusercontent.com/Kpoodle/Repo19B/master/addons.xml.md5</checksum>
        <datadir zip="true">https://raw.githubusercontent.com/Kpoodle/Repo19B/master/zips/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary></summary>
		<description></description>
		<platform>all</platform>
		<assets>
			<icon>icon.png</icon>
			<fanart>fanart.jpg</fanart>
		</assets>
	</extension>
</addon>

</addons>
